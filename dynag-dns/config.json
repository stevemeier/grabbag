{
	"server": {
		"listen": "127.0.0.1",
		"port": 5300
	},
	"names": [
		{ "name": "foo.example.org",
		  "command": "/usr/bin/true",
		  "interval": 60,
		  "rr": "60 IN A 1.2.3.4"
		},
		{ "name": "bar.example.org",
		  "command": "/usr/bin/false",
		  "interval": 30,
		  "rr": "60 IN A 5.6.7.8"
		},
		{ "name": "ipv6.example.org",
		  "command": "ls /",
		  "interval": 15,
		  "rr": "AAAA 2001:db8::1234"
		}
	]
}
